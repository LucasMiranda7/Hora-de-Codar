programa {
  funcao inicio() {

    //Retângulo
    
    //Variaveis do valor real
    real base
    real altura
    real area

    //entrada da Base
    escreva("Valor da Base do Retângulo: ")
    leia(base)

    //entrada da Altura
    escreva("Valor da Altura do Retângulo: ")
    leia(altura)

    //Calculo da área
    area = base * altura
    
    //Total
    escreva("A aréa do total do retângulo é: " + area)
  }
}
