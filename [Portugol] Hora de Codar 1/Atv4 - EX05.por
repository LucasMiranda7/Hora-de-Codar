programa {
  funcao inicio() {
    //Paralelogramo

    //Variaveis do tipo real
    real base
    real altura
    real area

    //Entrada
    escreva("Valor da base do Paralelogramo: ")
    leia(base)

    //Entrada
    escreva("Valor da altura do Paralelogramo: ")
    leia(altura)
    
    //Calculo
    area = base * altura

    //Valor total da área
    escreva("O total da área do Paralelograma é: " + area)
  }
}
