programa {
  funcao inicio() {
    //Quadrado

    //Variaveis do valor real
    real lado
    real area

    //Entrada
    escreva("Valor do lado do quadrado: ")
    leia(lado)

    //Calculo
    area = lado * lado
    
    //Resultado final
    escreva("O valor total da area do Quadrado é: " + area + "m²")
    
  }
}
